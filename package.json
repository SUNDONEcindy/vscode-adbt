{
	"name": "adbt",
	"displayName": "ADBT",
	"publisher": "igordvlpr",
	"description": "ðŸ“œ Provides ADBT (Adblock template) support in Visual Studio Code. ðŸª…",
	"version": "1.0.0",
	"engines": {
		"vscode": "^1.80.0"
	},
	"categories": [
		"Programming Languages"
	],
	"activationEvents": [
		"onLanguage:adbt"
	],
	"files": [
		"dist",
		"meta"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"configurationDefaults": {
			"[adbt]": {
				"files.eol": "\n"
			}
		},
		"languages": [
			{
				"id": "adbt",
				"aliases": [
					"Adblock template",
					"adbt"
				],
				"extensions": [
					".adbt"
				]
			}
		],
		"grammars": [
			{
				"language": "adbt",
				"scopeName": "source.adbt",
				"path": "./syntaxes/adbt.tmLanguage.json"
			}
		],
		"jsonValidation": [
			{
				"fileMatch": [
					"*.adbt.meta"
				],
				"url": "./meta/adbt-meta-schema.json"
			}
		]
	},
	"scripts": {
		"build": "tsc -p ./"
	},
	"devDependencies": {
		"@types/node": "^16.17.1",
		"@types/vscode": "^1.80.0",
		"typescript": "^5.1.6"
	}
}
